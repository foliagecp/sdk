# Cluster mode: Set to true for a failover cluster with multiple nodes, false for a single node
NATS_CLUSTER_MODE=true
# Should be odd
NATS_REPLICAS=3
# NATS URLs: For cluster mode, provide a comma-separated list of node URLs; for single mode, provide one URL
NATS_URL=nats://nats:foliage@nats1:4222,nats://nats:foliage@nats2:4222,nats://nats:foliage@nats3:4222
# NATS_URL=nats://nats:foliage@nats:4222  # Example for single node (uncomment and adjust as needed)

MASTER_FUNC_CONTEXT_INCREMENT=true
MASTER_FUNC_CONTEXT_INCREMENT_OPTION=1
MASTER_FUNC_OBJECT_CONTEXT_PROCESS=false
MASTER_FUNC_JS_PLUGIN=true
MASTER_FUNC_LOGS=true

CREATE_SIMPLE_GRAPH_TEST=false
REQUEST_REPLY_TEST=false
KV_MUTICES_TEST=false
TRIGGERS_TEST=true

KV_MUTICES_TEST_DURATION_SEC=10
KV_MUTICES_TEST_WORKERS=4
